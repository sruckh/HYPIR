[flake8]
max-line-length = 127
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    .venv,
    .eggs,
    *.egg-info,
    .tox,
    .coverage,
    .pytest_cache,
    .mypy_cache,
    .DS_Store,
    assets,
    results,
    outputs,
    experiments,
    .gradio,
    temp
per-file-ignores =
    __init__.py:F401
    predict.py:E501
max-complexity = 10
ignore = 
    E203,
    E501,
    W503,
    F403,
    F405